{
  "name": "my chrome extension",
  "version": "1.0.0",
  "description": "my very first extension",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*.youtube.com/*"
      ]
    }
  ],
  "permissions": [
    "bookmarks"
  ]
}